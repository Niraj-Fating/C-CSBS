#include <stdio.h>

int main() 
{   
    int i,sum;
    int a[5] ,b[5],c[5];
    
    
    printf("Enter any 5 Numbers to be Added :\n");
    sum = 0;
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }
    
    printf("Enter any 5 Numbers to be Added :\n");
    
    for(i=0;i<5;i++)
    {
        scanf("%d",&b[i]);
    }
    
    for(i=0;i<5;i++)
    {   
         sum= c[i] = a[i] + b[i];
    
    printf("Sum of Elements of Array %d and %d is : %d\n",a[i],b[i],sum);
    }
}
