#include <stdio.h>                           
#include <stdlib.h>                          

int main()
{
int d1,d2,d3,d4,d5,num,sum,ori;             
printf ("In Inter any 5 digit Number: ");  
scanf("%d",&num);                                                  
ori=num;                                     

d1=num%10;                                   
num=num/10;

d2=num%10;                                   
num=num/10;

d3=num%10;                                   
num=num/10;

d4=num%10;                                  
num=num/10;

d5=num%10;

sum = d1 + d2 + d3 + d4 + d5;                     

printf("In Sum of digit of number %d is %d ",ori,sum);
}
