#include <stdio.h>

int main() 
{
    int a = 5,b = 10;
    printf("\n A = %d B = %d",a,b);
    b = a++;
    
    printf("\n A = %d B = %d",a++);
    printf("\n A = %d B = %d",++b);
    printf("\n A = %d B = %d",a--);
    printf("\n A = %d B = %d",--b);
    printf("\n A = %d B = %d",a,b);
    return 0;
}
