#include <stdio.h>

int main() 
{   
    int i,j;
    int a[3][3];
    int b[3][3];
    int c[3][3];

    printf("Enter  3 x 3 Matrix A  :\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    
    printf("Enter  3 x 3 Matrix B  :\n");
    
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }


    printf("Matrix A is:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ",a[i][j]);
        }printf("\n");
    }printf("\n");
    
    
    printf("Matrix A is:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ",b[i][j]);
        }printf("\n");
    }printf("\n");
    
    
    printf("Matrix C is:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            c[i][j] = a[i][j] + b[i][j];
        }printf("\n");
    }
    
    
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ",c[i][j]);
        }printf("\n");
    }printf("\n");
    
}
