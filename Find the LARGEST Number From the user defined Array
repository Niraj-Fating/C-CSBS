#include <stdio.h>

int main() 
{   
    int i,max;
    int a[10];
    
    
    printf("Enter any 10 Numbers :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    
    max = a[0];
    
    for(i=0;i<10;i++)
    {   
        if(a[i]>max)
        {
            max = a[i];
        }
    }   
        printf("The largest Number is %d \n",max);
    
}
