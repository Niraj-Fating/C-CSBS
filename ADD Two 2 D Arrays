#include <stdio.h>
int main() 
{
    int i,j,sum;
    int a[3][3];
    int b[3][3];
    int c[3][3];
    
     printf("\nEnter any 3x3 Matrix");
     
     for(i=0;i<3;i++)
     {
         for(j=0;j<3;j++)
         {
             scanf("%d",&a[i][j]);
         }printf("\n");
     }
     printf("\nEntered matrix is \n");
     
     for(i=0;i<3;i++)
     {
         for(j=0;j<3;j++)
         {
             printf("%d \t",b[i][j]);
         }printf("\n");
     }printf("\n");
     
     printf("\nEnter any 3x3 Matrix");
     
     for(i=0;i<3;i++)
     {
         for(j=0;j<3;j++)
         {
             scanf("%d",&b[i][j]);
         }printf("\n");
     }
     printf("\nEntered matrix is \n");
     
     for(i=0;i<3;i++)
     {
         for(j=0;j<3;j++)
         {
             printf("%d \t",a[i][j]);
         }printf("\n");
     }printf("\n");
     
     for(i=0;i<3;i++)
     {
         for(j=0;j<3;j++)
         {  sum=c[i][j]=a[i][j]+b[i][j];
             printf("%d  \t \t",sum);
         }printf("\n");
     }printf("\n");
}
