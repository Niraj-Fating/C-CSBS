#include <stdio.h>

int main() 
{   
    int i,count;
    int a[15];
    
    
    printf("Enter any 15 Numbers :\n");
    for(i=0;i<15;i++)
    {
        scanf("%d",&a[i]);
    }
    
    count = 0;
    
    for(i=0;i<15;i++)
    {   
        if(a[i] == 0)
        {
            count++;
        }
    }   
        printf("Total Number of ZEROES are : %d \n",count);
    
}
